#26
class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        if len(nums) < 2:
            return len(nums)
        else:
            i = 1
            newLength = 1
            while i < len(nums):
                if nums[i] != nums[i-1]:
                    newLength+=1
                    nums[newLength-1] = nums[i]
                i+=1;
            return newLength
